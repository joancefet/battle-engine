  },
  "dependencies" : {
    "underscore":"*"
  },
  "devDependencies" : {
    "mocha" : "*",
    "expect.js" : "*",
    "browserify" : "*",
    "coveralls": "*",
    "istanbul": "*",
    "mocha-lcov-reporter": "*"
  }
}
